                                <li class="nav-icon-btn nav-icon-btn-danger dropdown">

                                    <a href="#notifications" class="dropdown-toggle" data-toggle="dropdown">

                                        <i class="nav-icon fa fa-cog"></i>

                                        <span class="small-screen-text">Setting</span>

                                    </a>

                                    <ul class="dropdown-menu">

                                        <li><a href="#"><i class="fa fa-user"></i>&nbsp;&nbsp;@User.Identity.Name <text>'s Profile</text></a></li>
                                        <li>
                                            @{
                                                int pos = Array.IndexOf(ViewBag.user.roles, "TISSUE BANK SUPER ADMIN");
                                                if (pos > -1)
                                                {
                                                    if (ViewBag.user.InfoId == 0)
                                                    {
                                                        <a href="@Url.Action("Index", "TissueBankProfile", new { Area = "TissueBank" })">
                                                            <i class="dropdown-icon fa fa-user-plus"></i>
                                                            &nbsp;&nbsp;Add Tissue Bank
                                                        </a>
                                                    }
                                                    else
                                                    {
                                                        <a href="@Url.Action("Index", "TissueBankProfile", new { Area = "TissueBank" })">
                                                            <i class="dropdown-icon fa fa-user-plus"></i>
                                                            &nbsp;&nbsp;Manage Tissue Bank Profile
                                                        </a>
                                                    }
                                                }
                                            }


                                        </li>

                                        <li class="divider"></li>

                                        <li><a href="#"><i class="dropdown-icon fa fa-cog"></i>&nbsp;&nbsp;Account Settings</a></li>
                                        <li>
                                            @if (ViewBag.user.InfoId > 0)
                                            {
                                                pos = Array.IndexOf(ViewBag.user.roles, "TISSUE BANK SUPER ADMIN");
                                                if (pos > -1)
                                                {
                                                    <a href="@Url.Action("Index", "User", new { Area = "TissueBank" })">
                                                        <i class="dropdown-icon fa fa-user-plus"></i>
                                                        &nbsp;&nbsp;User Management
                                                    </a>
                                                }
                                            }
                                        </li>
                                    </ul>
                                </li>
                                <li class="nav-icon-btn nav-icon-btn-danger">
                                    <a href="@Url.Action("LogOut", "Account" , new { Area="" })">
