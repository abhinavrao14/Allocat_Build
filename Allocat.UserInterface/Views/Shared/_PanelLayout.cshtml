<body class="@(ViewBag.user.InfoId == 0 ? "theme-default main-menu-animated animate-mm-sm animate-mm-md animate-mm-lg animate-mm-sm animate-mm-md animate-mm-lg mmc" : "theme-default main-menu-animated animate-mm-sm animate-mm-md animate-mm-lg")">

    <div id="main-wrapper">
