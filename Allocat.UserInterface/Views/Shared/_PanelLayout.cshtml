                                            <i class="menu-icon fa fa-list-alt"></i>
                                            <span class="mm-text mmc-dropdown-delay animated fadeIn">Reports Panel</span>
                                        </a>
                                        <ul class="mmc-dropdown-delay animated fadeInLeft">
                                            <li>
                                                <a href="@Url.Action("Index", "RequestForQuote", new { Area = "TissueBank" })"><span class="mm-text mmc-dropdown-delay animated fadeIn">RFQ</span></a>
                                            </li>
                                            <li>
                                                <a tabindex="-1" href="@Url.Action("Index", "Order", new { Area = "TissueBank" })"><span class="mm-text">Purchase Order</span></a>
                                            </li>
                                        </ul>

                                    </li>
                                }
                                else
                                {
                                    pos = Array.IndexOf(ViewBag.user.roles, "TISSUE BANK FULFILMENT MANAGER");
                                    if (pos > -1)
                                    {
                                        <li class="mm-dropdown mm-dropdown-root">
                                            <a href="#">
                                                <i class="menu-icon fa fa-list-alt"></i>
                                                <span class="mm-text mmc-dropdown-delay animated fadeIn">Reports Panel</span>
                                            </a>
                                            <ul class="mmc-dropdown-delay animated fadeInLeft">
                                                <li>
                                                    <a href="@Url.Action("Index", "RequestForQuote", new { Area = "TissueBank" })"><span class="mm-text mmc-dropdown-delay animated fadeIn">RFQ</span></a>
