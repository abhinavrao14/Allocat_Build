                            <div>
                                <div class="text-bg"><span class="text-semibold">Welcome,</span><br /><span class="text-slim">@User.Identity.Name</span></div>
                                <img src="~/Assets/image/user.png" alt="" class="" style="border:2px solid #ffffff;" />
                            </div>

                        </div>

                        <ul class="navigation">

                            <li class="active">

                                <a href="#"><i class="menu-icon fa fa-dashboard"></i><span class="mm-text mmc-dropdown-delay animated fadeIn">Dashboard</span></a>

                            </li>
                            @{
                                pos = Array.IndexOf(ViewBag.user.roles, "TISSUE BANK SUPER ADMIN");
