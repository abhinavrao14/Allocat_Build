                                        init.push(function () {

                                            $('#main-navbar-notifications').slimScroll({ height: 250 });

                                        });

                                    </script>
                                    @if (ViewBag.user.InfoId > 0)
                                    {
                                        <div class="dropdown-menu widget-notifications no-padding" style="width: 300px">

                                            <div style="position: relative; overflow: hidden; width: auto; height: 250px;" class="slimScrollDiv">
                                                <div style="overflow: hidden; width: auto; height: 250px;" class="notifications-list" id="main-navbar-notifications">
                                                    <div class="notification">
                                                        <div class="notification-description">You have <strong>9</strong> new orders.</div>

                                                        <div class="notification-ago">12h ago</div>

                                                        <div class="notification-icon fa fa-history bg-default"></div>

                                                    </div> <!-- / .notification -->


                                                    <div class="notification">

                                                        <div class="notification-description">You have <strong>9</strong> new orders.</div>

                                                        <div class="notification-ago">12h ago</div>

                                                        <div class="notification-icon fa fa-history bg-default"></div>

                                                    </div> <!-- / .notification -->



                                                    <div class="notification">

                                                        <div class="notification-description">You have <strong>9</strong> new orders.</div>

                                                        <div class="notification-ago">12h ago</div>

                                                        <div class="notification-icon fa fa-history bg-default"></div>

                                                    </div> <!-- / .notification -->



                                                    <div class="notification">

                                                        <div class="notification-description">You have <strong>9</strong> new orders.</div>

                                                        <div class="notification-ago">12h ago</div>

                                                        <div class="notification-icon fa fa-history bg-default"></div>

                                                    </div> <!-- / .notification -->




                                                </div>



                                                <div style="background: rgb(0, 0, 0) none repeat scroll 0% 0%; width: 7px; position: absolute; top: 0px; opacity: 0.4; display: block; border-radius: 7px; z-index: 99; right: 1px;" class="slimScrollBar"></div><div style="width: 7px; height: 100%; position: absolute; top: 0px; display: none; border-radius: 7px; background: rgb(51, 51, 51) none repeat scroll 0% 0%; opacity: 0.2; z-index: 90; right: 1px;" class="slimScrollRail"></div>
                                            </div> <!-- / .notifications-list -->

                                            <a href="#" class="notifications-link">MORE NOTIFICATIONS</a>

                                        </div> <!-- / .dropdown-menu -->
