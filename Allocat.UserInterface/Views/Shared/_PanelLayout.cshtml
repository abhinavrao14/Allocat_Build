                            @{
                                pos = Array.IndexOf(ViewBag.user.roles, "TISSUE BANK SUPER ADMIN");
                                if (pos > -1)
                                {
                                    <li>
                                        <a href="@Url.Action("Index", "Product" , new { Area="TissueBank" })"><i class="menu-icon fa fa-cog"></i><span class="mm-text mmc-dropdown-delay animated fadeIn">Product Management</span></a>
                                    </li>
                                }
                                else
                                {
                                    pos = Array.IndexOf(ViewBag.user.roles, "TISSUE BANK INVENTORY MANAGER");
                                    if (pos > -1)
                                    {
                                        <li>
                                            <a href="@Url.Action("Index", "Product" , new { Area="TissueBank" })"><i class="menu-icon fa fa-cog"></i><span class="mm-text mmc-dropdown-delay animated fadeIn">Product Management</span></a>
                                        </li>
                                    }
                                }
                            }

                        </ul> <!-- / .navigation -->
                    </div> <!-- / #main-menu-inner -->
                    <div style="background: rgb(0, 0, 0) none repeat scroll 0% 0%; width: 7px; position: absolute; top: 0px; opacity: 0.4; display: none; border-radius: 7px; z-index: 99; right: 1px; height: 741px;" class="slimScrollBar"></div><div style="width: 7px; height: 100%; position: absolute; top: 0px; display: none; border-radius: 7px; background: rgb(51, 51, 51) none repeat scroll 0% 0%; opacity: 0.2; z-index: 90; right: 1px;" class="slimScrollRail"></div>
                </div>

            </div> <!-- / #main-menu -->
        <!-- /4. $MAIN_MENU -->
                                }

