        <div id="main-menu-bg"></div>
    </div>




    <script type="text/javascript">

        init.push(function () {
            // Javascript code here
        })
        window.PixelAdmin.start(init);
    </script>
    @RenderSection("scripts", required: false)
    @Scripts.Render("~/bundles/app")
