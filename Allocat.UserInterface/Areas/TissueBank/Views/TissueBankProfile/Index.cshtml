    @*for Tissue Bank Update*@
    @if (@ViewBag.user.InfoId > 0)
    {

        <div class="panel-heading" id="headerstyle">
            Manage Tissue Bank Profile
        </div>

        <form class="form-horizontal" name="formTissueBankDetail" novalidate ng-submit="UpdateTissueBankDetail()">
            <div class="row">
                <div class="col-md-6">
