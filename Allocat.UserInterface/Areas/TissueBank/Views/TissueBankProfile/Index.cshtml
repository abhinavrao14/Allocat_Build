                                    <div class="col-sm-6">
                                        <div class="form-group no-margin-hr">
                                            <label class="control-label">AATB License Number</label>
                                            <span class="error error-msg" ng-show="formTissueBank.AATBLicenseNumber.$error.required">*</span>
                                            <input type="text" class="form-control" ng-required="true" name="AATBLicenseNumber" ng-model="tissueBank.AATBLicenseNumber" ng-pattern="AATBLicenseNumberPattern" ng-maxlength="aATBLicenseNumberMaxLength" placeholder="Enter AATB License Number" />
                                            <span class="error error-msg" ng-show="formTissueBank.AATBLicenseNumber.$error.maxlength">AATB License Number must be upto 30 digits</span><br />
