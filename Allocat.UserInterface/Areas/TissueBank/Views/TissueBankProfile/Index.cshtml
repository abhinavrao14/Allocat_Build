                                            <label class="control-label"><span class="error error-msg" ng-show="formTissueBank.isAgree.$error.required">*</span> I agree that $25 will be deducted from my credit card as Registration-Fee.</label><br />
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="panel-footer text-center">
                <input type="submit" class="btn btn-info" value="Submit" ng-disabled="formTissueBank.$invalid" />
                @{
                    var ViewUrl = Url.Action("Index", "Home", new { Area = "" });
                    ViewUrl = HttpUtility.UrlDecode(ViewUrl);
                }
                <a data-ng-href="@ViewUrl" style="display:inline"><button type="button" class="btn btn-danger">Cancel</button></a>
            </div>
        </form>
                    }

